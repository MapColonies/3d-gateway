{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": {
        "__name": "TELEMETRY_METRICS_ENABLED",
        "__format": "boolean"
      },
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "externalServices": {
    "storeTrigger": "STORE_TRIGGER_URL",
    "catalog": "CATALOG_URL",
    "lookupTables": {
      "url": "LOOKUP_TABLES_URL",
      "subUrl": "LOOKUP_TABLES_SUB_URL"
    }
  },
  "paths": {
    "basePath": "BASE_PATH",
    "pvPath": "PV_SOURCE_PATH"
  },
  "validation": {
    "percentageLimit": "PERCENTAGE_LIMIT"
  },
  "provider": "PROVIDER_TO",
  "S3": {
    "accessKeyId": "S3_DEST_ACCESS_KEY_ID",
    "secretAccessKey": "S3_DEST_SECRET_ACCESS_KEY",
    "endpointUrl": "S3_DEST_END_POINT_URL",
    "bucket": "S3_DEST_BUCKET",
    "region": "S3_DEST_REGION",
    "forcePathStyle": {
      "__name": "S3_DEST_FORCE_PATH_STYLE",
      "__format": "boolean"
    },
    "sslEnabled": {
      "__name": "S3_DEST_SSL_ENABLED",
      "__format": "boolean"
    },
    "maxAttempts": {
      "__name": "S3_DEST_MAX_ATTEMPTS",
      "__format": "number"
    }
  }
}
