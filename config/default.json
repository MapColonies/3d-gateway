{
  "openapiConfig": {
    "filePath": "./bundledApi.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8082",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "storeTrigger": {
    "url": "http://127.0.0.1:8081"
  },
  "lookupTables": {
    "url": "http://127.0.0.1:8085",
    "subUrl": "lookup-tables/lookupData"
  },
  "catalog": {
    "url": "http://127.0.0.1:8081",
    "subUrl": "metadata"
  },
  "paths": {
    "basePath": "/home/3DModels",
    "pvPath": "/NewYork"
  },
  "validation": {
    "percentageLimit": 10
  }
}
